$ TMA TSUPREM4 NMOS transistor simulation
$   Part a:  Through field oxidation

$ Define the grid
MESH       GRID.FAC=0.5
$METHOD     ERR.FAC=2.0

$ Read the mask definition file
MASK       IN.FILE=pmos.tl1 

$ Initialize the structure
INITIALIZE <100>  BORON=1E14


$ n-well
DEPOSIT    PHOTORESIST  NEGATIVE  THICKNESS=1
EXPOSE     MASK=Nwell
DEVELOP
$ETCH OXIDE

IMPLANT    Arsenic      DOSE=6E11  ENERGY=50  
ETCH       PHOTORESIST  ALL
DIFFUSION  TIME=30  TEMP=1300 INERT           

$ Initial oxidation
DIFFUSION  TIME=30  TEMP=1250  DRY HCL=5


$ Nitride deposition and field region mask
DEPOSIT    PHOTORESIST  POSITIVE  THICKNESS=1
EXPOSE     MASK=Gate
DEVELOP
ETCH       OXIDE        TRAP  THICKNESS=1.5 UNDERCUT=0.1
ETCH       PHOTORESIST  ALL

DEPOSIT    PHOTORESIST  POSITIVE  THICKNESS=1
EXPOSE     MASK=Doping
DEVELOP

$ Boron field implant
IMPLANT    Boron  DOSE=6E12  ENERGY=50
ETCH       PHOTORESIST  ALL
DIFFUSION  TIME=30  TEMP=950

DEPOSIT MAT=POLY THICKNESS=0.3
DEPOSIT PHOTORESIST POSITIVE THICKNESS=1.0
EXPOSE MASK=Gate_Contact
DEVELOP
ETCH POLYSILICON TRAP THICK=2
ETCH PHOTORESIST ALL

DEPOSIT ALUMINUM THICKNESS=0.5
DEPOSIT PHOTORESIST POSITIVE THICKNESS=1.0
EXPOSE MASK=Contact
DEVELOP
ETCH ALUMINUM TRAP THICKNESS=1.5
ETCH PHOTORESIST ALL

ELECTRIC X=5 THRESHOLD PMOS V="-3 0 0.05"

$ Save structure
SAVEFILE   OUT.FILE=PMOS

savefile out.f=pmos.tif tif
